{
  "manifest_version": 3,
  "name": "60-30-10 Palette New Tab",
  "version": "1.0.0",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "description": "A chrome extension in honour of the 603010 design principle",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/paletteIcon.png"
  },
  "icons": {
    "128": "icons/paletteIcon.png"
  },
  "permissions": ["storage"]
}